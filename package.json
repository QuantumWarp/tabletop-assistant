{
  "name": "tabletop-assistant",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "homepage": "https://quantumwarp.github.io/tabletop-assistant",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@iconify/react": "^4.1.1",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "@reduxjs/toolkit": "^2.0.1",
    "immer": "^10.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.2",
    "react-rnd": "^10.4.1",
    "react-router-dom": "^6.20.1",
    "react-timeago": "^7.2.0",
    "redux-persist": "^6.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@types/react-redux": "^7.1.32",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-timeago": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
